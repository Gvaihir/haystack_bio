      "def check_file(filename):\r",
      "\r\n",
      "    try:\r",
      "\r\n",
      "        with open(filename): pass\r",
      "\r\n",
      "    except IOError:\r",
      "\r\n",
      "        error('I cannot open the file:'+filename)\r",
      "\r\n",
      "        sys.exit(1)\r",
      "\r\n",
      "\r",
      "\r\n",
      "def which(program):\r",
      "\r\n",
      "    import os\r",
      "\r\n",
      "    def is_exe(fpath):\r",
      "\r\n",
      "        return os.path.isfile(fpath) and os.access(fpath, os.X_OK)\r",
      "\r\n",
      "\r",
      "\r\n",
      "    fpath, fname = os.path.split(program)\r",
      "\r\n",
      "    if fpath:\r",
      "\r\n",
      "        if is_exe(program):\r",
      "\r\n",
      "            return program\r",
      "\r\n",
      "    else:\r",
      "\r\n",
      "        for path in os.environ[\"PATH\"].split(os.pathsep):\r",
      "\r\n",
      "            path = path.strip('\"')\r",
      "\r\n",
      "            exe_file = os.path.join(path, program)\r",
      "\r\n",
      "            if is_exe(exe_file):\r",
      "\r\n",
      "                return exe_file\r",
      "\r\n",
      "\r",
      "\r\n",
      "    return None\r",
      "\r\n",
      "\r",
      "\r\n",
      "\r",
      "\r\n",
      "def query_yes_no(question, default=\"yes\"):\r",
      "\r\n",
      "    valid = {\"yes\":True,   \"y\":True,  \"ye\":True,\r",
      "\r\n",
      "             \"no\":False,     \"n\":False}\r",
      "\r\n",
      "    if default == None:\r",
      "\r\n",
      "        prompt = \" [y/n] \"\r",
      "\r\n",
      "    elif default == \"yes\":\r",
      "\r\n",
      "        prompt = \" [Y/n] \"\r",
      "\r\n",
      "    elif default == \"no\":\r",
      "\r\n",
      "        prompt = \" [y/N] \"\r",
      "\r\n",
      "    else:\r",
      "\r\n",
      "        raise ValueError(\"invalid default answer: '%s'\" % default)\r",
      "\r\n",
      "\r",
      "\r\n",
      "    while True:\r",
      "\r\n",
      "        sys.stdout.write(question + prompt)\r",
      "\r\n",
      "        choice = raw_input().lower()\r",
      "\r\n",
      "        if default is not None and choice == '':\r",
      "\r\n",
      "            return valid[default]\r",
      "\r\n",
      "        elif choice in valid:\r",
      "\r\n",
      "            return valid[choice]\r",
      "\r\n",
      "        else:\r",
      "\r\n",
      "            sys.stdout.write(\"Please respond with 'yes' or 'no' \"\\\r",
      "\r\n",
      "                             \"(or 'y' or 'n').\\n\")\r",
      "\r\n",
      "\r",
      "\r\n",
      "\r",
      "\r\n",
